{
    "explorer.confirmDelete": false,
    "workbench.startupEditor": "newUntitledFile",
    "explorer.confirmDragAndDrop": false,
    "editor.wordWrap": "on",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "workbench.iconTheme": "vscode-icons",
    "vsicons.dontShowNewVersionMessage": true,
    "editor.fontFamily": "Hack Nerd Font",
    "editor.fontLigatures": false,
    "editor.minimap.enabled": true,
    "editor.renderControlCharacters": false,
    "editor.renderWhitespace": "none",
    "editor.bracketPairColorization.enabled": true,
    "workbench.colorCustomizations": {
        "activityBar.background": "#000000",
        "activityBar.border": "#000000",
        "activityBar.inactiveForeground": "#777777",
        "activityBar.foreground": "#00ff00",
        "statusBar.background": "#000000",
        "tab.activeBackground": "#000000",
        "tab.inactiveBackground": "#000000",
        "tab.border": "#000000",
        "tab.activeBorder": "#00ff00",
        "tab.hoverBorder": "#ff0000",
        "panel.background": "#000000",
        "editor.background": "#000000",
        "editorLineNumber.activeForeground": "#ffffff",
        "editorLineNumber.foreground": "#999999",
        "scrollbar.shadow": "#000000",
        "panelTitle.activeForeground": "#ffffff",
        "panelTitle.activeBorder": "#00ff00",
        "panelTitle.inactiveForeground": "#999999",
        "sideBarSectionHeader.background": "#000000",
        "sideBarSectionHeader.border": "#333333",
        "sideBar.background": "#000000",
        "sideBar.border": "#000000",
        "titleBar.activeBackground": "#000000",
        "titleBar.activeForeground": "#ffffff",
        "titleBar.inactiveForeground": "#999999",
        "titleBar.border": "#000000",
        "editorBracketHighlight.foreground1": "#d74177",
        "editorBracketHighlight.foreground2": "#38adae",
        "editorBracketHighlight.foreground3": "#c172d9",
        "editorBracketHighlight.foreground4": "#fca5f1",
        "editorBracketHighlight.foreground5": "#97c26c",
        "editorBracketHighlight.foreground6": "#00b7ff",
        "terminal.background": "#000000",
        "terminal.tab.activeBorder": "#00ff00",
        "editorBracketHighlight.unexpectedBracket.foreground": "#db6165",
        "editorCursor.foreground": "#ff0000",

        // Background color for lines that got inserted/removed. 
        // The color must not be opaque so as not to hide underlying decorations.
        "diffEditor.insertedLineBackground": "#00ff0022",
        "diffEditor.removedLineBackground": "#ff000033",
        "diffEditorGutter.insertedLineBackground": "#00ff0022",
        "diffEditorGutter.removedLineBackground": "#ff000033",
        "diffEditorOverview.insertedForeground": "#00ff00AA",
        "diffEditorOverview.removedForeground": "#ff0000AA",
    },
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "comment",
                "settings": {
                    "foreground": "#666666"
                }
            },
            {
                "scope": "string.quoted.docstring",
                "settings": {
                    "foreground": "#cc77ff",
                    "fontStyle": "italic"
                }
            },
            {
                "scope": "keyword.control",
                "settings": {
                    "foreground": "#ff00ff",
                    "fontStyle": "italic"
                }
            },
            {
                "scope": [
                    "keyword",
                    "keyword.operator.arithmetic",
                    "keyword.operator",
                    "keyword.operator.assignment",
                    "keyword.operator.logical",
                    "keyword.operator.new",
                    "keyword.other"
                ],
                "settings": {
                    "foreground": "#ff00ff"
                }
            },
            {
                "scope": "string",
                "settings": {
                    "foreground": "#ff7f50"
                }
            },
            {
                "scope": [
                    "constant.numeric",
                    "constant.numeric.float",
                    "constant.numeric.hex",
                    "constant.numeric.integer",
                    "constant.numeric.octal"
                ],
                "settings": {
                    "foreground": "#ffffff"
                }
            },
            {
                "scope": [
                    "variable",
                    "variable.name",
                    "variable.other",
                    "variable.parameter",
                    "variable.language"
                ],
                "settings": {
                    "foreground": "#00ff00"
                }
            },
            {
                "scope": [
                    "entity.name.function",
                    "entity.name.method"
                ],
                "settings": {
                    "foreground": "#ffff00"
                }
            },
            {
                "scope": "entity.name.type",
                "settings": {
                    "foreground": "#00ffff"
                }
            }
        ]
    },
    "terminal.integrated.defaultProfile.osx": "zsh",
    "terminal.integrated.profiles.osx": {
        "zsh": {
            "path": "/bin/zsh",
            "args": [
                "-l"
            ]
        }
    },
    // "terminal.integrated.fontFamily": "Fira Mono for Powerline",
    "terminal.integrated.fontFamily": "Hack Nerd Font",
    // "terminal.integrated.fontFamily": "Consolas",
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.cursorBlinking": true,
    "terminal.external.osxExec": "Warp.app",
    "gitlens.advanced.messages": {
        "suppressFileNotUnderSourceControlWarning": true,
        "suppressGitMissingWarning": true,
        "suppressGitVersionWarning": true,
        "suppressLineUncommittedWarning": true
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "gitlens.views.repositories.files.layout": "list",
    "breadcrumbs.enabled": true,
    "git.ignoreLegacyWarning": true,
    "security.workspace.trust.untrustedFiles": "open",
    "jupyter.askForKernelRestart": false,
    "python.defaultInterpreterPath": "/opt/anaconda3/envs/ai/bin/python",
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "diffEditor.maxComputationTime": 0,
    "editor.inlineSuggest.enabled": true,
    "editor.unicodeHighlight.invisibleCharacters": false,
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "liveServer.settings.CustomBrowser": "chrome:PrivateMode",
    "liveServer.settings.donotShowInfoMsg": true,
    // "rewrap.autoWrap.enabled": true,
    // "python.sortImports.args": [
    //     // "-m", "GRID",
    //     // "--sl"
    //     "--ensure-newline-before-comments",
    //     "--use-parentheses",
    //     "--force-grid-wrap", "0",
    //     // "--include-trailing-comma",
    //     "--line-length", "119",
    //     "--lines-after-imports", "2",
    //     // "--multi-line-output", "3",
    // ],
    "editor.largeFileOptimizations": false,
    "[python]": {
        "editor.formatOnType": true
    },
    "gitlens.rebaseEditor.ordering": "asc",
    "files.autoSave": "afterDelay",
    "jupyter.widgetScriptSources": [
        "jsdelivr.com",
        "unpkg.com"
    ],
    "jupyter.interactiveWindow.textEditor.executeSelection": true,
    "git.confirmSync": false,
    "yaml.customTags": [
        "!And",
        "!And sequence",
        "!If",
        "!If sequence",
        "!Not",
        "!Not sequence",
        "!Equals",
        "!Equals sequence",
        "!Or",
        "!Or sequence",
        "!FindInMap",
        "!FindInMap sequence",
        "!Base64",
        "!Join",
        "!Join sequence",
        "!Cidr",
        "!Ref",
        "!Sub",
        "!Sub sequence",
        "!GetAtt",
        "!GetAZs",
        "!ImportValue",
        "!ImportValue sequence",
        "!Select",
        "!Select sequence",
        "!Split",
        "!Split sequence"
    ],
    "isort.args": [
        "ensure_newline_before_comments=True",
        "force_grid_wrap=0",
        "include_trailing_comma=True",
        "lines_after_imports=2",
        "multi_line_output=3",
        "use_parentheses=True",
        "profile=black"
    ],
    "cmake.configureOnOpen": true,
    "workbench.editorAssociations": {
        "git-rebase-todo": "gitlens.rebase"
    },
    "CodeGPT.apiKey": "Ollama",
    "CodeGPT.model": "mixtral-8x7b-fw-chat",
    "CodeGPT.Autocomplete.provider": "Ollama - codellama",
    "diffEditor.ignoreTrimWhitespace": false
}