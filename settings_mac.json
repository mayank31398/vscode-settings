{
    "explorer.confirmDelete": false,
    "workbench.startupEditor": "newUntitledFile",
    "explorer.confirmDragAndDrop": false,
    "editor.wordWrap": "on",
    "files.autoSave": "afterDelay",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "diffEditor.ignoreTrimWhitespace": false,
    "workbench.iconTheme": "vscode-icons",
    "vsicons.dontShowNewVersionMessage": true,
    "editor.fontFamily": "Consolas, 'Courier New', monospace",
    "editor.fontLigatures": false,
    "editor.minimap.enabled": true,
    "editor.renderControlCharacters": false,
    "editor.renderWhitespace": "none",
    "editor.bracketPairColorization.enabled": true,
    "workbench.colorCustomizations": {
        "activityBar.background": "#000000",
        "activityBar.border": "#000000",
        "activityBar.inactiveForeground": "#777777",
        "activityBar.foreground": "#00ff00",
        "statusBar.background": "#000000",
        "tab.activeBackground": "#000000",
        "tab.inactiveBackground": "#000000",
        "tab.border": "#000000",
        "tab.activeBorder": "#00ff00",
        "tab.hoverBorder": "#ff0000",
        "editor.background": "#000000",
        "editorLineNumber.activeForeground": "#ffffff",
        "editorLineNumber.foreground": "#999999",
        "scrollbar.shadow": "#000000",
        "panelTitle.activeForeground": "#ffffff",
        "panelTitle.activeBorder": "#00ff00",
        "panelTitle.inactiveForeground": "#999999",
        "sideBar.background": "#000000",
        "sideBar.border": "#000000",
        "titleBar.activeBackground": "#000000",
        "titleBar.activeForeground": "#ffffff",
        "titleBar.inactiveForeground": "#999999",
        "titleBar.border": "#000000",
        "editorBracketHighlight.foreground1": "#d74177",
        "editorBracketHighlight.foreground2": "#38adae",
        "editorBracketHighlight.foreground3": "#c172d9",
        "editorBracketHighlight.foreground4": "#fca5f1",
        "editorBracketHighlight.foreground5": "#97c26c",
        "editorBracketHighlight.foreground6": "#00b7ff",
        "editorBracketHighlight.unexpectedBracket.foreground": "#db6165"
    },
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "comment",
                "settings": {
                    "foreground": "#666666"
                }
            },
            {
                "scope": "keyword.control",
                "settings": {
                    "foreground": "#ff00ff",
                    "fontStyle": "italic"
                }
            },
            {
                "scope": [
                    "keyword",
                    "keyword.operator.arithmetic",
                    "keyword.operator",
                    "keyword.operator.assignment",
                    "keyword.operator.logical",
                    "keyword.operator.new",
                    "keyword.other"
                ],
                "settings": {
                    "foreground": "#ff00ff"
                }
            },
            {
                "scope": "string",
                "settings": {
                    "foreground": "#d2691e"
                }
            },
            {
                "scope": [
                    "constant.numeric",
                    "constant.numeric.float",
                    "constant.numeric.hex",
                    "constant.numeric.integer",
                    "constant.numeric.octal"
                ],
                "settings": {
                    "foreground": "#ffffff"
                }
            },
            {
                "scope": [
                    "variable",
                    "variable.name",
                    "variable.other",
                    "variable.parameter",
                    "variable.language"
                ],
                "settings": {
                    "foreground": "#00ff00"
                }
            },
            {
                "scope": [
                    "entity.name.function",
                    "entity.name.method"
                ],
                "settings": {
                    "foreground": "#ffff00"
                }
            },
            {
                "scope": "entity.name.type",
                "settings": {
                    "foreground": "#00ffff"
                }
            }
        ]
    },
    "java.errors.incompleteClasspath.severity": "ignore",
    "java.semanticHighlighting.enabled": true,
    "terminal.integrated.defaultProfile.osx": "zsh",
    "terminal.integrated.profiles.osx": {
        "bash": {
            "path": "bash",
            "args": [
                "-l"
            ],
            "icon": "terminal-bash"
        },
        "zsh": {
            "path": "/bin/zsh",
            "args": [
                "-l"
            ]
        },
        "fish": {
            "path": "fish",
            "args": [
                "-l"
            ]
        },
        "tmux": {
            "path": "tmux",
            "icon": "terminal-tmux"
        },
        "pwsh": {
            "path": "pwsh",
            "icon": "terminal-powershell"
        }
    },
    "terminal.integrated.fontFamily": "Fira Mono for Powerline",
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.cursorBlinking": true,
    "terminal.external.osxExec": "iTerm.app",
    "gitlens.advanced.messages": {
        "suppressFileNotUnderSourceControlWarning": true,
        "suppressGitMissingWarning": true,
        "suppressGitVersionWarning": true,
        "suppressLineUncommittedWarning": true
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "gitlens.views.repositories.files.layout": "list",
    "breadcrumbs.enabled": true,
    "git.ignoreLegacyWarning": true,
    "security.workspace.trust.untrustedFiles": "open",
